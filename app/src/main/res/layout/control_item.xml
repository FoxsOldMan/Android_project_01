<?xml version="1.0" encoding="utf-8"?><!--Used for zhaohu fragment.-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:background="@android:color/white">

		<ImageView
			android:id="@+id/app_image"
			android:layout_width="48dp"
			android:layout_height="48dp"
			android:layout_margin="12dp"
			android:layout_centerHorizontal="false"
			android:src="@mipmap/ic_launcher" />

		<TextView
			android:id="@+id/app_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:singleLine="true"
			android:layout_centerVertical="true"
			android:text="app_name"
			android:layout_toLeftOf="@id/app_result"
			android:layout_toRightOf="@+id/app_image" />

		<TextView
			android:id="@+id/app_result"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_centerVertical="true"
			android:layout_marginLeft="8dp"
			android:singleLine="true"
			android:text="analyze_result" />

</RelativeLayout>